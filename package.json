    "prettier": "^1.7.4",
    "puppeteer": "^0.12.0",
    "react-hot-loader": "^3.1.1",
    "react-test-renderer": "^16.0.0",
    "style-loader": "^0.19.0",
    "webpack": "^3.8.1",
    "webpack-dashboard": "0.4.0",
    "webpack-dev-server": "^2.9.2",
    "webpack-merge": "^4.1.0"
  }

{
  "name": "react-redux-pokedex",
  "version": "0.0.1",
  "description": "This is example of Pokedex with React-Redux with Redux-saga.",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dashboard webpack-dev-server",
    "build": "npm run clean && cross-env NODE_ENV=production webpack-dashboard webpack --progress --hide-modules",
    "clean": "rimraf 'dist/*.{js,json,svg,png,css,woff,woff2,ttf,eot,html}'",
    "tree": "tree -I node_modules",
    "test": "jest",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "flow": "flow",
    "lint": "eslint src/js/**/*",
    "lint-fix": "eslint src/js/**/*.jsx src/js/**/*.js src/js/**/* --fix --quiet",
    "browserTest": "node browserTest/sample.js",
    "flow-coverage": "flow-coverage-report -i 'src/**/*.js' -x 'src/**/*.spec.js' -x 'src/js/component/adultPC/**' -x 'src/js/component/general/**' -x 'src/**/*.jsx' -x 'src/**/style.js' -t html -t json -t text"
  },
  "jest": {
    "setupFiles": [
      "./test/shim.js",
      "./test/jestsetup.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/takahiro-saeki/React-Redux-Pokedex.git"
  },
  "author": "takahiro-saeki",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/takahiro-saeki/React-Redux-Pokedex/issues"
  },
  "homepage": "https://github.com/takahiro-saeki/React-Redux-Pokedex#readme",
  "dependencies": {
    "@researchgate/react-intersection-observer": "^0.4.0",
    "isomorphic-fetch": "^2.2.1",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-md-spinner": "^0.2.5",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.16.0",
    "styled-components": "^2.2.1",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "@storybook/react": "^3.2.12",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-module-resolver": "2.7.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-function-bind": "^6.22.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "cross-env": "^5.1.0",
    "css-loader": "^0.28.7",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.2",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-import-resolver-babel-module": "^4.0.0-beta.3",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.4.0",
    "file-loader": "^1.1.5",
    "flow-bin": "^0.57.3",
    "flow-coverage-report": "^0.3.0",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^21.2.1",
    "jest-styled-components": "^4.7.1",
    "postcss-loader": "^2.0.8",
    "postcss-size": "^2.0.0",
    "pre-commit": "^1.2.2",
